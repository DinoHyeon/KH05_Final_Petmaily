<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- typeAlias는 DTO 경로를 축약 해 준다. -->
<!-- alias="사용이름" type="클래스 경로" -->
<configuration>
	<typeAliases>
		<typeAlias alias="BoardDTO" type="com.petmily.dto.BoardDTO" />
		<typeAlias alias="DiseaseDTO" type="com.petmily.dto.DiseaseDTO" />
		<typeAlias alias="MemberDTO" type="com.petmily.dto.MemberDTO" />
		<typeAlias alias="QuizDTO" type="com.petmily.dto.QuizDTO" />
		<typeAlias alias="ReplyDTO" type="com.petmily.dto.ReplyDTO" />
	</typeAliases>
</configuration>   